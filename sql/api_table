use blackgraph;

create table tl_fdmtis (
    secID varchar(30),
    endDate date not null,
    publishDate date,
    endDateRep date,
    partyID int,
    ticker varchar(10) references report_info(ticker),
    secShortName varchar(30),
    exchangeCD varchar(30),
    actPubtime varchar(30),
    mergedFlag varchar(30),
    reportType varchar(30) not null,
    fiscalPeriod varchar(30),
    accoutingStandards varchar(30),
    currencyCD varchar(30),
    tRevenue double,
    revenue double,
    intIncome double,
    intExp double,
    premEarned double,
    commisIncome double,
    commisExp double,
    TCogs double,
    COGS double,
    premRefund double,
    NCompensPayout double,
    reserInsurContr double,
    policyDivPayt double,
    reinsurExp double,
    bizTaxSurchg double,
    sellExp double,
    adminExp double,
    finanExp double,
    assetsImpairLoss double,
    fValueChgGain double,
    investIncome double,
    AJInvestIncome double,
    forexGain double,
    operateProfit double,
    NoperateIncome double,
    NoperateExp double,
    NCADisploss double,
    TProfit double,
    incomeTax double,
    NIncome double,
    NIncomeAttrP double,
    minorityGain double,
    basicEPS double,
    dilutedEPS double,
    othComprIncome double,
    TComprIncome double,
    comprIncAttrP double,
    comprIncAttrMS double,
    PRIMARY KEY(ticker,endDate,reportType)
);
