use blackgraph;

create table IF NOT EXISTS tl_fdmtis (
    secID varchar(30),
    endDate date not null,
    publishDate date,
    endDateRep date,
    partyID int,
    ticker varchar(10) references report_info(ticker),
    secShortName varchar(30),
    exchangeCD varchar(30),
    actPubtime varchar(30),
    mergedFlag varchar(30),
    reportType varchar(30) not null,
    fiscalPeriod varchar(30),
    accoutingStandards varchar(30),
    currencyCD varchar(30),
    tRevenue double,
    revenue double,
    intIncome double,
    intExp double,
    premEarned double,
    commisIncome double,
    commisExp double,
    TCogs double,
    COGS double,
    premRefund double,
    NCompensPayout double,
    reserInsurContr double,
    policyDivPayt double,
    reinsurExp double,
    bizTaxSurchg double,
    sellExp double,
    adminExp double,
    finanExp double,
    assetsImpairLoss double,
    fValueChgGain double,
    investIncome double,
    AJInvestIncome double,
    forexGain double,
    operateProfit double,
    NoperateIncome double,
    NoperateExp double,
    NCADisploss double,
    TProfit double,
    incomeTax double,
    NIncome double,
    NIncomeAttrP double,
    minorityGain double,
    basicEPS double,
    dilutedEPS double,
    othComprIncome double,
    TComprIncome double,
    comprIncAttrP double,
    comprIncAttrMS double,
    PRIMARY KEY(ticker,endDate,reportType)
);

create table IF NOT EXISTS tl_fdmtbs (
    secID varchar(30),
    endDate date not null,
    publishDate date,
    endDateRep date,
    partyID int,
    ticker varchar(10) references report_info(ticker),
    secShortName varchar(30),
    exchangeCD varchar(30),
    actPubtime varchar(30),
    mergedFlag varchar(30),
    reportType varchar(30) not null,
    fiscalPeriod varchar(30),
    accoutingStandards varchar(30),
    currencyCD varchar(30),
    cashCEquiv double,
    settProv double,
    loanToOthBankFi double,
    tradingFA double,
    NotesReceiv double,
    AR double,
    prepayment double,
    premiumReceiv double,
    reinsurReceiv double,
    reinsurReserReceiv double,
    intReceiv double,
    divReceiv double,
    othReceiv double,
    purResaleFa double,
    inventories double,
    NCAWithin1Y double,
    othCA double,
    TCA double,
    disburLA double,
    availForSaleFa double,
    htmInvest double,
    LTReceive double,
    LTEquityInvest double,
    investRealEstate double,
    fixedAssets double,
    CIP double,
    constMaterials double,
    fixedAssetsDisp double,
    producBiolAssets double,
    oilAndGasAssets double,
    intanAssets double,
    RD double,
    goodwill double,
    LTAmorExp double,
    deferTaxAssets double,
    othNCA double,
    TNCA double,
    TAssets double,
    STBorr double,
    CBBorr double,
    depos double,
    loanFrOthBankFi double,
    tradingFL double,
    NotesPayable double,
    AP double,
    advanceReceipts double,
    soldForRepurFa double,
    commisPayable double,
    payrollPayable double,
    taxesPayable double,
    intPayable double,
    divPayable double,
    othPayable double,
    reinsurPayable double,
    insurReser double,
    fundsSecTradAgen double,
    fundsSecUndwAgen double,
    NCLWithin1Y double,
    othCL double,
    TCL double,
    LTBorr double,
    bondPayable double,
    LTPayable double,
    specificPayables double,
    estimatedLiab double,
    deferTaxLiab double,
    othNCL double,
    TNCL double,
    TLiab double,
    paidInCapital double,
    capitalReser double,
    treasuryShare double,
    specialReser double,
    surplusReser double,
    ordinRiskReser double,
    retainedEarnings double,
    forexDiffer double,
    TEquityAttrP double,
    minorityInt double,
    TShEquity double,
    TLiabEquity double,
    PRIMARY KEY(ticker,endDate,reportType)
);


create table IF NOT EXISTS tl_fdmtisbank(
    reportType varchar(30) not null,
    secID varchar(30),
    endDate date,
    publishDate date,
    endDateRep date,
    partyID int,
    ticker varchar(10) references report_info(ticker),
    secShortName varchar(30),
    exchangeCD varchar(30),
    actPubtime varchar(30),
    mergedFlag varchar(30),
    fiscalPeriod varchar(30),
    accoutingStandards varchar(30),
    currencyCD varchar(30),
    revenue double,
    NIntIncome double,
    intIncome double,
    intExp double,
    NCommisIncome double,
    commisIncome double,
    commisExp double,
    othOperRev double,
    COGS double,
    bizTaxSurchg double,
    genlAdminExp double,
    assetsImpairLoss double,
    othOperCosts double,
    fValueChgGain double,
    investIncome double,
    AJInvestIncome double,
    forexGain double,
    operateProfit double,
    NoperateIncome double,
    NoperateExp double,
    TProfit double,
    incomeTax double,
    NIncome double,
    NIncomeAttrP double,
    minorityGain double,
    basicEPS double,
    dilutedEPS double,
    othComprIncome double,
    TComprIncome double,
    comprIncAttrP double,
    comprIncAttrMS double,
    PRIMARY KEY(ticker,endDate,reportType)
);

